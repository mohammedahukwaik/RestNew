@model IEnumerable<RestNew.Models.TransactionContactUs>

@{
    ViewData["Title"] = "Index";
}



<div class="pagetitle">
    <nav>
        <ol class="breadcrumb">
            <li class="breadcrumb-item"><a asp-action="Index" asp-controller="Home">Home</a></li>
            <li class="breadcrumb-item active"> Transaction ContactUs</li>
        </ol>
    </nav>
</div><!-- End Page Title -->




<section class="section">
    <div class="row">
        <div class="col-lg-12">

            <div class="card">
                <div class="card-body">

                    <!-- Table with stripped rows -->
                    <table class="table datatable">
                        <thead>
                            <tr>
                                <th>
                                    @Html.DisplayNameFor(model => model.TransactionContactUsFullName)
                                </th>
                                <th>
                                    @Html.DisplayNameFor(model => model.TransactionContactUsEmail)
                                </th>
                                <th>
                                    @Html.DisplayNameFor(model => model.TransactionContactUsSubject)
                                </th>
                                <th>
                                    @Html.DisplayNameFor(model => model.TransactionContactUsMessage)
                                </th>
                            </tr>
                        </thead>
                        <tbody>
                            @foreach (var item in Model)
                            {
                                <tr>
                                    <td>
                                        @Html.DisplayFor(modelItem => item.TransactionContactUsFullName)
                                    </td>
                                    <td>
                                        @Html.DisplayFor(modelItem => item.TransactionContactUsEmail)
                                    </td>
                                    <td>
                                        @Html.DisplayFor(modelItem => item.TransactionContactUsSubject)
                                    </td>
                                    <td>
                                        @Html.DisplayFor(modelItem => item.TransactionContactUsMessage)
                                    </td>
                                </tr>
                            }
                        </tbody>
                    </table>
                    <!-- End Table with stripped rows -->

                </div>
            </div>

        </div>
    </div>
</section>

